<application>
  <component name="MessDetector">
    <phpmd_settings>
      <MessDetectorConfiguration tool_path="C:/php/phpmd" />
    </phpmd_settings>
  </component>
  <component name="PhpCodeSniffer">
    <phpcs_settings>
      <PhpCSConfiguration tool_path="C:/php/phpcs" />
    </phpcs_settings>
  </component>
  <component name="PhpDebugGeneral" xdebug_notify_resolved_breakpoints="false" />
</application>